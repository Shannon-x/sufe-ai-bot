# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here

# Cloudflare AI Gateway Configuration
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_GATEWAY_NAME=your_gateway_name

# Redis Configuration (External Redis)
# 如果使用外部Redis，请配置以下参数
REDIS_HOST=localhost        # Redis服务器地址
REDIS_PORT=6379            # Redis端口，默认6379
REDIS_PASSWORD=            # Redis密码（如果有）
REDIS_DB=0                 # Redis数据库编号，默认0

# Optional: Grafana Configuration
GRAFANA_PASSWORD=admin     # Grafana管理员密码