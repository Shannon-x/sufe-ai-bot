# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here

# AI API Configuration
# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key

# Custom API Endpoint
CUSTOM_API_URL=http://localhost:8080/v1
CUSTOM_API_KEY=your_custom_api_key

# Redis Configuration (External Redis)
# 如果使用外部Redis，请配置以下参数
REDIS_HOST=localhost        # Redis服务器地址
REDIS_PORT=6379            # Redis端口，默认6379
REDIS_PASSWORD=            # Redis密码（如果有）
REDIS_DB=0                 # Redis数据库编号，默认0

# Optional: Grafana Configuration
GRAFANA_PASSWORD=admin     # Grafana管理员密码